{
  "projectType": "library",
  "root": "libs/firestarter/app",
  "sourceRoot": "libs/firestarter/app/src",
  "prefix": "material-workspace",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/firestarter/app"],
      "options": {
        "jestConfig": "libs/firestarter/app/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/firestarter/app/src/**/*.ts",
          "libs/firestarter/app/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
